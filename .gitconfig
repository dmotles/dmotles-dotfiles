[user]
	name = Daniel Motles
	email = dan.motles@intermedix.com
[color]
	status = auto
	branch = auto
	diff = auto
	interactive = true
[core]
	excludesfile = /Users/dmotles/.gitignore_global
	editor = vim
	whitespace = trailing-space,space-before-tab
[merge]
	tool = vimdiff
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = matching
